{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/www",
    "dev": "clear && cross-env NODE_ENV=development DEBUG=appFreeDocs nodemon ./src/bin/www",
    "alltest": "cross-env NODE_ENV=test mocha */test/*.test.js --timeout 10000",
    "test": "cross-env NODE_ENV=test mocha --timeout 10000",
    "heroku-postbuild": "yarn run migrate-mongo up",
    "migrate": "yarn run migrate-mongo",
    "deleteall": "cross-env NODE_ENV=development node ./src/seed/deleteall.js",
    "seed": "cross-env NODE_ENV=development node ./src/seed/seed.js",
    "deleteseed": "cross-env NODE_ENV=development node ./src/seed/deleteseed.js",
    "groupseed": "cross-env NODE_ENV=development node ./src/seed/groupseed.js",
    "deletegroupseed": "cross-env NODE_ENV=development node ./src/seed/deletegroupseed.js"
  },
  "dependencies": {
    "basic-auth": "^2.0.1",
    "bcrypt": "^5.0.1",
    "clean-stacktrace": "^1.1.0",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.1.4",
    "jade": "~1.11.0",
    "joi": "^17.4.0",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "migrate-mongo": "^8.2.2",
    "mongoose": "^5.11.18",
    "mongoose-aggregate-paginate-v2": "^1.0.42",
    "mongoose-paginate-v2": "^1.3.17",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "chai": "^4.3.3",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "mocha": "^8.3.0",
    "supertest": "^6.1.3"
  },
  "events": {
    "start": "clear"
  }
}